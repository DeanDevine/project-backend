      "description": "String - REQUIRED",
      "price": "Number - REQUIRED",
      "quantity": "Number - DEFAULT 0 - REQUIRED"
    },
    "exampleResponse": {
      "_id": "64c7f62faf6ea105749979d5",
      "item_name": "new shop item",
      "description": "item description",
